{
  "expo": {
    "name": "WalletSlot",
    "slug": "WalletSlot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wallet.png",
    "scheme": "walletslot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/images/wallet.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "WalletSlot"
    },
    "ios": {
      "icon": "./assets/images/wallet.png",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "bundleIdentifier": "com.walletslot.app"
    },
    "android": {
      "icon": "./assets/images/wallet.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wallet.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.walletslot.app",
      "versionCode": 1,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/wallet.png",
          "color": "#ffffff",
          "sounds": [],
          "mode": "production"
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          }
        }
      ],
      "@react-native-firebase/messaging"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5ad0a526-ea05-4611-a7d0-23a44ed158a5"
      }
    }
  }
}
